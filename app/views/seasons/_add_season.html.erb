<%= form_for([current_club, @season]) do |f| %>

<div class="form-group">
  <%= f.label :year %><br>
  <%= f.number_field :year, class: "form-control" %>
</div>
<%= f.hidden_field :club_id, value: current_club.id %>

<div class="action">
  <input type="submit" name="commit" value="New Season" class="btn btn-success">
</div>

<% end %>
