<div class="container-fluid new_club">
	<div class="col-md-6 col-md-offset-3 club_form">
		<div class="club_form_h1">
			<h1>Club Login</h1>
		</div>
			<%= form_for :session, url: sessions_path do |f| %>
			  <div class="form-group">
			    <%= f.text_field :club_name, class: "form-control", placeholder: "Club Name" %>
			  </div>
				<div class="form-group">
			    <%= f.password_field :password, class: "form-control", placeholder: "Password" %>
			  </div>
			  <div class="form-group">
			    <%= f.password_field :password_confirmation, class: "form-control", placeholder: "Password Confirmation" %>
			  </div>
				<div class="actions">
				  <%= submit_tag "Log In" %><br />
				</div>
			<% end %>
	</div>
</div>
