<%= form_for Club.new, url: new_club_path do |f| %>

<div class="form-group">
  <%= f.text_field :club_name, class: "form-control", placeholder: "Club Name" %>
</div>
<div class="form-group">
  <%= f.file_field :logo, class: "form-control", label: "Upload Club Logo" %>
</div>
<div class="form-group">
  <%= f.text_field :sport, class: "form-control", placeholder: "Sport" %>
</div>
<div class="form-group">
  <%= f.text_field :director, class: "form-control",  placeholder: "Director" %>
</div>
<div class="form-group">
  <%= f.text_field :email,  class: "form-control", placeholder: "Email" %>
</div>
<div class="form-group">
  <%= f.password_field :password, class: "form-control", placeholder: "Password" %>
</div>
<div class="form-group">
  <%= f.password_field :password_confirmation,  class: "form-control", placeholder: "Password Confirmation" %>
</div>
<div class="actions">
  <input type="submit" name="commit" value="Create Club">
</div>

<% end %>
