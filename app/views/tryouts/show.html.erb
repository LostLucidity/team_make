<div id="wrapper">
  <nav class="navbar navbar-default navbar-cls-top " role="navigation" style="margin-bottom: 0">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="index.html"><%= current_club.club_name.capitalize %></a>
    </div>
    <div style="color: white;
    padding: 15px 50px 5px 50px;
    float: right;
    font-size: 16px;"> Making Teams Since : <%= current_club.created_at.strftime("%m-%d-%Y") %> &nbsp; <%= link_to 'Log out', logout_path , :class => "btn btn-danger square-btn-adjust"%> </div>
  </nav>
  <!-- /. NAV TOP  -->
  <nav class="navbar-default navbar-side" role="navigation">
    <div class="sidebar-collapse">
      <ul class="nav" id="main-menu">
        <li class="text-center">
          <%= image_tag current_club.logo(:medium), class:"img-circle" %>
        </li>
        <li>
          <!-- #replace with Link_to -->
          <%= link_to '<i class="fa fa-tachometer"> &nbsp; Manage Club</i>'.html_safe, club_season_path(current_club, current_season)%>

        </li>
        <li>
          <!-- #replace with Link_to -->
          <%= link_to '<i class="fa fa-wrench">  &nbsp;  Update Club Info</i>'.html_safe, edit_club_path(current_club, current_season)%>
        </li>
        <li>
          <!-- #replace with Link_to -->
          <%= link_to '<i class="fa fa-wrench">  &nbsp;  Post Tryout</i>'.html_safe,  new_club_season_tryout_path(current_club, @season)%>
        </li>
        <li>
          <!-- #replace with Link_to -->
          <%= link_to '<i class="fa fa-wrench">  &nbsp; View Seasons</i>'.html_safe,  club_seasons_path(current_club)%>
        </li>
        <li>
          <!-- #replace with Link_to -->
          <%= link_to '<i class="fa fa-users">  &nbsp; View All Tryouts</i>'.html_safe,  club_season_tryouts_path(current_club, @season)%>
        </li>
        <li>
          <!-- #replace with Link_to -->
          <a><i class="fa fa-trash-o"></i>Delete Club Account</a>
        </li>
      </ul>
    </div>
  </nav>
  <!-- /. NAV SIDE  -->
  <div id="page-wrapper" >
    <div id="page-inner" class = "pre-scrollable" style="max-height: 100px">

      <!-- /. ROW  -->
      <div class="row">
        <div class="col-md-9 col-sm-12 col-xs-12">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4><li  class="fa fa-child"> Registered Athletes</li></h4>
            </div>
            <div class="panel-body">
              <table class="table table-striped table-hover ">
                <tr>
                  <th>Name</th>
                  <th>Age</th>
                  <th>Email</th>
                  <th>Phone</th>
                </tr>
                <% @tryout.athletes.all.each do |athlete| %>
                <tr>
                  <%# image_tag athlete.avatar(:icon)%>
                  <td><%= athlete.name.upcase %></td>
                  <td><%= athlete.age %></td>
                  <td><%= athlete.email.upcase %></td>
                  <td><%= athlete.telephone %></td>
                </tr>
                <%end%>
              </table>
            </div>
          </div>
        </div>
      </div>
      <hr />
    </div>
  </div>
  <!-- /. ROW  -->
</div>
<!-- /. PAGE INNER  -->
</div>
<!-- /. PAGE WRAPPER  -->
</div>
